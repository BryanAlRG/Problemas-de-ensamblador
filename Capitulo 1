.text
.global main

main:
    push {ip, lr}        @ Guardar registros

    mov r4, #9           @ Inicializar el contador con 9

loop:
    mov r0, r4           @ Mover el número actual a r0 para imprimirlo
    ldr r1, =format      @ Cargar la dirección del formato en r1
    bl printf            @ Llamar a printf

    add r4, r4, #1       @ Incrementar el contador
    cmp r4, #44          @ Comparar con 44 (43 + 1)
    bne loop             @ Si no es igual, continuar el bucle

    mov r0, #0           @ Código de salida 0
    pop {ip, pc}         @ Restaurar registros y retornar
